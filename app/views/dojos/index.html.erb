<h1>Dojos#index</h1>
<p>Find me in app/views/dojos/index.html.erb</p>
<h1>Listing <%= @dojos.count %> Dojos</h1>
<table>
<thead>
    <tr>
        <td>Coding Dojo Branch</td>
        <td>Street</td>
        <td>City</td>
        <td>State</td>
        <td>Actions</td>
    </tr>
</thead>
<tbody>
<% if @dojos %>
    <% @dojos.each do |dojo| %>
    <tr>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
        <td>
            <a href="/dojos/<%= dojo.id %>">Show</a>
            <a href="/dojos/<%= dojo.id %>/edit">Edit</a>
            <a href="/dojos/<%= dojo.id %>/destroy">Destroy</a>
        </td>
    </tr>
    <% end %>
<% end %>
</tbody>
</table>
<a href="/dojos/new">New Dojo</a>