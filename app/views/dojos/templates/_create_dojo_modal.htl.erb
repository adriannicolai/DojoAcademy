<form action="/dojos" method="post" id="newDojoForm">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <div class="form-group">
        <label>Branch:</label>
        <input type="text" class="form-control" name="dojo[branch]" id="newDojoBranch">
    </div>
    <div class="form-group">
        <label>Street:</label>
        <input type="text" class="form-control" name="dojo[street]" id="newDojoStreet">
    </div>
    <div class="form-group">
        <label>City:</label>
        <input type="text" class="form-control" name="dojo[city]" id="newDojoCity">
    </div>
    <div class="form-group">
    <label>State:</label>
        <input type="text" class="form-control" name="dojo[state]" id="newDojoState">
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary close-modal" data-dismiss="modal">Close</button>
        <input type="submit" class="btn btn-primary" value="Create">
    </div>
</form>